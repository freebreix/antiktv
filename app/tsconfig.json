{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "esnext",
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
  "types": ["svelte", "vite/client", "@sveltejs/kit"],
    "verbatimModuleSyntax": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "strict": true,
  "noUncheckedIndexedAccess": true,
  "resolveJsonModule": true
  },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.ts",
    "src/**/*.svelte"
  ]
}
